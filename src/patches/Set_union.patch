::host Set
::hvar <T>
::type (other: Set<T>) => Set<T>
::syms union or

::docn
Set union

::impl
function(other) {
  const result = new Set();
  for (const item of this) result.add(item);
  for (const item of other) result.add(item);
  return result;
}

::test
const A = new Set([-1, 0]);
const B = new Set([0, 1]);
const AuB = A[$.union](B);
expect(AuB).toStrictEqual(new Set([-1, 0, 1]));
